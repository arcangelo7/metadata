				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\begin{aligned} \lim _{t\rightarrow \infty } \frac{\mu (t)(D)}{\mu (t)(A)}= \lim _{t\rightarrow \infty } \frac{\mu (t)(D)}{\mu (t)(B)}\frac{\mu (t)(A)}{\mu (t)(B)}= 0; \end{aligned}$$\end{document}</tex-math><mml:math id="M42" display="block"><mml:mrow><mml:mtable><mml:mtr><mml:mtd columnalign="right"><mml:mrow><mml:munder><mml:mo movablelimits="true">lim</mml:mo><mml:mrow><mml:mi>t</mml:mi><mml:mo stretchy="false">&#x02192;</mml:mo><mml:mi>&#x0221e;</mml:mi></mml:mrow></mml:munder><mml:mfrac><mml:mrow><mml:mi>&#x003bc;</mml:mi><mml:mo stretchy="false">(</mml:mo><mml:mi>t</mml:mi><mml:mo stretchy="false">)</mml:mo><mml:mo stretchy="false">(</mml:mo><mml:mi>D</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mrow><mml:mi>&#x003bc;</mml:mi><mml:mo stretchy="false">(</mml:mo><mml:mi>t</mml:mi><mml:mo stretchy="false">)</mml:mo><mml:mo stretchy="false">(</mml:mo><mml:mi>A</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow></mml:mfrac><mml:mo>=</mml:mo><mml:munder><mml:mo movablelimits="true">lim</mml:mo><mml:mrow><mml:mi>t</mml:mi><mml:mo stretchy="false">&#x02192;</mml:mo><mml:mi>&#x0221e;</mml:mi></mml:mrow></mml:munder><mml:mfrac><mml:mrow><mml:mi>&#x003bc;</mml:mi><mml:mo stretchy="false">(</mml:mo><mml:mi>t</mml:mi><mml:mo stretchy="false">)</mml:mo><mml:mo stretchy="false">(</mml:mo><mml:mi>D</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mrow><mml:mi>&#x003bc;</mml:mi><mml:mo stretchy="false">(</mml:mo><mml:mi>t</mml:mi><mml:mo stretchy="false">)</mml:mo><mml:mo stretchy="false">(</mml:mo><mml:mi>B</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow></mml:mfrac><mml:mfrac><mml:mrow><mml:mi>&#x003bc;</mml:mi><mml:mo stretchy="false">(</mml:mo><mml:mi>t</mml:mi><mml:mo stretchy="false">)</mml:mo><mml:mo stretchy="false">(</mml:mo><mml:mi>A</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mrow><mml:mi>&#x003bc;</mml:mi><mml:mo stretchy="false">(</mml:mo><mml:mi>t</mml:mi><mml:mo stretchy="false">)</mml:mo><mml:mo stretchy="false">(</mml:mo><mml:mi>B</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow></mml:mfrac><mml:mo>=</mml:mo><mml:mn>0</mml:mn><mml:mo>&#x0037e;</mml:mo></mml:mrow></mml:mtd></mml:mtr></mml:mtable></mml:mrow></mml:math><graphic xlink:href="11538_2019_602_Article_Equ37.gif" position="anchor"/></alternatives></disp-formula>therefore (<inline-formula id="IEq18"><alternatives><tex-math id="M43">\documentclass[12pt]{minimal}
