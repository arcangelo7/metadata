				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\begin{aligned}&#x00026;\theta _i(x_i+1)\prod _{j= 1}^{x_i+1 - \alpha _i} \varphi _i(j)\nonumber \\&#x00026;\quad =\theta _i(x_i+1) \prod _{j=0}^{x_i+1-\alpha _i - 1} \varphi _i(x_i+1-\alpha _i - j)\qquad \quad \text {(by a rearrangement)}\nonumber \\&#x00026;\quad =\theta _i(x_i+1) \prod _{j=0}^{\lfloor (x_i+1-\alpha _i)/\alpha _i\rfloor -1} \theta _i((x_i+1-\alpha _i)-j\alpha _i)\quad \text {(by the inductive hypothesis)}\nonumber \\&#x00026;\quad = \theta _i(x_i+1) \prod _{j=0}^{\lfloor (x_i+1)/\alpha _i\rfloor -2} \theta _i(x_i+1-(j+1)\alpha _i)\nonumber \\&#x00026;\quad = \theta _i(x_i+1) \prod _{j=1}^{\lfloor (x_i+1)/\alpha _i\rfloor -1} \theta _i(x_i+1-j\alpha _i)\nonumber \\&#x00026;\quad = \prod _{j=0}^{\lfloor (x_i+1)/\alpha _i\rfloor -1} \theta _i(x_i+1-j\alpha _i), \end{aligned}$$\end{document}</tex-math><mml:math id="M262" display="block"><mml:mrow><mml:mtable columnspacing="0.5ex"><mml:mtr><mml:mtd/><mml:mtd columnalign="left"><mml:mrow><mml:msub><mml:mi mathvariant="italic">&#x003b8;</mml:mi><mml:mi>i</mml:mi></mml:msub><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:msub><mml:mi>x</mml:mi><mml:mi>i</mml:mi></mml:msub><mml:mo>+</mml:mo><mml:mn>1</mml:mn><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:munderover><mml:mo>&#x0220f;</mml:mo><mml:mrow><mml:mi>j</mml:mi><mml:mo>=</mml:mo><mml:mn>1</mml:mn></mml:mrow><mml:mrow><mml:msub><mml:mi>x</mml:mi><mml:mi>i</mml:mi></mml:msub><mml:mo>+</mml:mo><mml:mn>1</mml:mn><mml:mo>-</mml:mo><mml:msub><mml:mi mathvariant="italic">&#x003b1;</mml:mi><mml:mi>i</mml:mi></mml:msub></mml:mrow></mml:munderover><mml:msub><mml:mi mathvariant="italic">&#x003c6;</mml:mi><mml:mi>i</mml:mi></mml:msub><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mi>j</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow></mml:mrow></mml:mtd></mml:mtr><mml:mtr><mml:mtd columnalign="right"><mml:mrow/></mml:mtd><mml:mtd columnalign="left"><mml:mrow><mml:mspace width="1em"/><mml:mo>=</mml:mo><mml:msub><mml:mi mathvar