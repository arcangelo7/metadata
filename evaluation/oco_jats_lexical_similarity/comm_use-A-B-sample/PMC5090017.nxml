				\usepackage{amsfonts} 
				\usepackage{amssymb} 
				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\begin{aligned} I_C= &#x00026; {} \frac{\chi -\sqrt{\chi ^2 -P}}{2\beta (\mu +\nu +\sigma )} \end{aligned}$$\end{document}</tex-math><mml:math id="M234" display="block"><mml:mrow><mml:mtable columnspacing="0.5ex"><mml:mtr><mml:mtd columnalign="right"><mml:mrow><mml:msub><mml:mi>I</mml:mi><mml:mi>C</mml:mi></mml:msub><mml:mo>=</mml:mo></mml:mrow></mml:mtd><mml:mtd columnalign="left"><mml:mrow><mml:mrow/><mml:mfrac><mml:mrow><mml:mi mathvariant="italic">&#x003c7;</mml:mi><mml:mo>-</mml:mo><mml:msqrt><mml:mrow><mml:msup><mml:mi mathvariant="italic">&#x003c7;</mml:mi><mml:mn>2</mml:mn></mml:msup><mml:mo>-</mml:mo><mml:mi>P</mml:mi></mml:mrow></mml:msqrt></mml:mrow><mml:mrow><mml:mn>2</mml:mn><mml:mi mathvariant="italic">&#x003b2;</mml:mi><mml:mo stretchy="false">(</mml:mo><mml:mi mathvariant="italic">&#x003bc;</mml:mi><mml:mo>+</mml:mo><mml:mi mathvariant="italic">&#x003bd;</mml:mi><mml:mo>+</mml:mo><mml:mi mathvariant="italic">&#x003c3;</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow></mml:mfrac></mml:mrow></mml:mtd></mml:mtr></mml:mtable></mml:mrow></mml:math><graphic xlink:href="11538_2016_217_Article_Equ14.gif" position="anchor"/></alternatives></disp-formula>
<disp-formula id="Equ15"><label>15</label><alternatives><tex-math id="M235">\documentclass[12pt]{minimal}
				\usepackage{amsmath}
