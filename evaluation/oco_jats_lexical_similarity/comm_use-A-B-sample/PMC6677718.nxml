				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\begin{aligned} {{\mathbf {X}}}(t)= &#x00026; {} {{\mathbf {X}}}(0) + \sum _{k=2}^{K_s\alpha } \sum _{i=1}^N \Bigg \{ \int _0^t \frac{D_i}{\varDelta x^2} \left( X_i^k(s)-X_i^{k-1}(s)\right) \,\text{ d }s\nonumber \\&#x00026;\qquad \qquad \qquad \qquad + \int _0^t \sqrt{\frac{D_i}{\varDelta x^2}\left( X_i^{k}(s)+X_i^{k-1}(s)\right) {\chi }_{\left\{ X_i^k(s)+X_i^{k-1}(s)\ge 0\right\} }} \, \text{ d }W_i^{k-1}(s) \Bigg \} \,{\varvec{\nu }}_{-,i}^k \nonumber \\&#x00026;+ \sum _{k=K_s\alpha +2}^{K} \sum _{i=1}^N Y^k_{-,i} \left( \int _0^t \frac{D_i}{h^2} \, X_i^k(s)\, \text{ d }s \right) {\varvec{\nu }}_{-,i}^{k} \nonumber \\&#x00026;+ \sum _{k=K_s\alpha +1}^{K-1} \sum _{i=1}^N Y^{k}_{+,i} \left( \int _0^t \frac{D_i}{h^2} \, X_i^{k}(s)\, \text{ d }s \right) {\varvec{\nu }}_{+,i}^{k} \nonumber \\&#x00026;+ \underbrace{ \sum _{i=1}^N Y^{K_s\alpha +1}_{-,i} \left( \int _0^t \varPsi _1 \frac{D_i}{h^2} \, X_i^{K_s\alpha +1}(s)\,\text{ d }s \right) {\varvec{\nu }}_{-,i}^{K_s\alpha +1}}_{\text {Markov chain }\rightarrow \text { SPDE}} \nonumber \\&#x00026;+ \underbrace{ \sum _{i=1}^N Y^{K_s\alpha }_{+,i} \left( \int _0^t \varPsi _2 \frac{D_i}{\varDelta x^2} \, X_i^{K_s\alpha }(s) {\chi }_{\left\{ X_i^{K_s\alpha }(s)\ge 1 \right\} } \, \text{ d }s \right) {\varvec{\nu }}_{+,i}^{K_s\alpha } }_{\text {SPDE }\rightarrow \text { Markov chain}}. \end{aligned}$$\end{document}</tex-math><mml:math id="M374" display="block"><mml:mrow><mml:mtable><mml:mtr><mml:mtd columnalign="right"><mml:mrow><mml:mi mathvariant="bold">X</mml:mi><mml:mo stretchy="false">(</mml:mo><mml:mi>t</mml:mi><mml:mo stretchy="false">)</mml:mo><mml:mo>=</mml:mo></mml:mrow></mml:mtd><mml:mtd columnalign="left"><mml:mrow><mml:mrow/><mml:mi mathvariant="bold">X</mml:mi><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mn>0</mml:mn><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mo>+</mml:mo><mml:munderover><mml:mo>&#x02211;</mml:mo><mml:mrow><mml:mi>k</mml:mi><mml:mo>=</mml:mo><mml:mn>2</mml:mn