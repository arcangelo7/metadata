				\usepackage{amssymb} 
				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\begin{aligned} {{\mathbf {X}}}(t)= &#x00026; {} {{\mathbf {X}}}(0) + \sum _{k=2}^{K_s\alpha } \sum _{i=1}^N \Bigg \{ \int _0^t \frac{D_i}{\varDelta x^2} \left( X_i^k(s)-X_i^{k-1}(s)\right) \,\text{ d }s\nonumber \\&#x00026;\qquad \qquad \qquad \qquad + \int _0^t \sqrt{\frac{D_i}{\varDelta x^2}\left( X_i^{k}(s)+X_i^{k-1}(s)\right) {\chi }_{\left\{ X_i^k(s)+X_i^{k-1}(s)\ge 0\right\} }} \, \text{ d }W_i^{k-1}(s) \Bigg \} \,{\varvec{\nu }}_{-,i}^k \nonumber \\&#x00026;+ \sum _{k=K_s\alpha +2}^{K} \sum _{i=1}^N Y^k_{-,i} \left( \int _0^t \frac{D_i}{h^2} \, X_i^k(s) \, \text{ d }s \right) {\varvec{\nu }}_{-,i}^{k} \nonumber \\&#x00026;+ \sum _{k=K_s\alpha +1}^{K-1} \sum _{i=1}^N Y^{k}_{+,i} \left( \int _0^t \frac{D_i}{h^2} \, X_i^{k}(s)\, \text{ d }s \right) {\varvec{\nu }}_{+,i}^{k} \nonumber \\&#x00026;+ \underbrace{ \sum _{\ell =1}^{\alpha } \sum _{i=1}^N Y^{K_s\alpha +1}_{-,i} \left( \int _0^t \frac{D_i}{h^2} \, X_i^{K_s\alpha +1}(s)\, \text{ d }s \right) {\mathscr {I}}_{-,i}^{\ell }(t) \, {\varvec{\eta }}_{-,i}^{\ell } }_{\text {Markov chain }\rightarrow \text { SPDE}} \nonumber \\&#x00026;+ \underbrace{ \sum _{\ell =1}^{\alpha } \sum _{i=1}^N Y^{K_s\alpha }_{+,i} \left( \int _0^t \frac{D_i}{h^2} \sum _{j=1}^{\alpha } X_i^{(K_s-1)\alpha +j}(s) {\chi }_{\left\{ \sum _{j=1}^{\alpha } X_i^{(K_s-1)\alpha +j}(s)\ge 1\right\} } \, \text{ d }s \right) {\mathscr {I}}_{+,i}^{\ell }(t) \, {\varvec{\eta }}_{+,i}^{\ell } }_{\text {SPDE }\rightarrow \text { Markov chain}},\nonumber \\ \end{aligned}$$\end{document}</tex-math><mml:math id="M294" display="block"><mml:mrow><mml:mtable><mml:mtr><mml:mtd columnalign="right"><mml:mrow><mml:mi mathvariant="bold">X</mml:mi><mml:mo stretchy="false">(</mml:mo><mml:mi>t</mml:mi><mml:mo stretchy="false">)</mml:mo><mml:mo>=</mml:mo></mml:mrow></mml:mtd><mml:mtd columnalign="left"><mml:mrow><mml:mrow/><mml:mi mathvariant="bold">X</mml:mi><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mn>0</mml:mn><mml:mo stretchy="fals