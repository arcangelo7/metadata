&#x0044c;&#x0043d;&#x0043e;&#x00435; &#x00437;&#x00434;&#x00440;&#x00430;&#x00432;&#x0043e;&#x0043e;&#x00445;&#x00440;&#x00430;&#x0043d;&#x00435;&#x0043d;&#x00438;&#x00435; &#x0043d;&#x00435;&#x00441;&#x00435;&#x00442; &#x0043f;&#x00440;&#x00430;&#x00432;&#x00438;&#x00442;&#x00435;&#x0043b;&#x0044c;&#x00441;&#x00442;&#x00432;&#x0043e;, &#x00447;&#x00442;&#x0043e;&#x00431;&#x0044b; &#x00432; &#x0043e;&#x00441;&#x0043d;&#x0043e;&#x00432;&#x0043d;&#x0043e;&#x0043c; &#x00443;&#x0043c;&#x00435;&#x0043d;&#x0044c;&#x00448;&#x00438;&#x00442;&#x0044c; &#x00444;&#x00438;&#x0043d;&#x00430;&#x0043d;&#x00441;&#x0043e;&#x00432;&#x0044b;&#x00435; &#x0043f;&#x00440;&#x00435;&#x0043f;&#x0044f;&#x00442;&#x00441;&#x00442;&#x00432;&#x00438;&#x0044f; &#x0043a; &#x0043f;&#x0043e;&#x0043b;&#x00443;&#x00447;&#x00435;&#x0043d;&#x00438;&#x0044e; &#x0043c;&#x00435;&#x00434;&#x00438;&#x00446;&#x00438;&#x0043d;&#x00441;&#x0043a;&#x00438;&#x00445; &#x00443;&#x00441;&#x0043b;&#x00443;&#x00433; &#x00431;&#x00435;&#x00434;&#x0043d;&#x0044b;&#x0043c;&#x00438; &#x00441;&#x0043b;&#x0043e;&#x0044f;&#x0043c;&#x00438; &#x0043d;&#x00430;&#x00441;&#x00435;&#x0043b;&#x00435;&#x0043d;&#x00438;&#x0044f;. &#x0041d;&#x00430;&#x00441;&#x00435;&#x0043b;&#x00435;&#x0043d;&#x00438;&#x00435; &#x00422;&#x00430;&#x00438;&#x0043b;&#x00430;&#x0043d;&#x00434;&#x00430; &#x00441;&#x00442;&#x00430;&#x00440;&#x00435;&#x00435;&#x00442;, &#x00438; &#x0043f;&#x00440;&#x0043e;&#x00444;&#x00438;&#x0043b;&#x0044c; &#x00437;&#x00430;&#x00431;&#x0043e;&#x0043b;&#x00435;&#x00432;&#x00430;&#x0043d;&#x00438;&#x00439; &#x0043c;&#x00435;&#x0043d;&#x0044f;&#x00435;&#x00442;&#x00441;&#x0044f; &#x0043f;&#x0043e; &#x0043c;&#x00435;&#x00440;&#x00435; &#x00441;&#x0043e;&#x00432;&#x00435;&#x00440;&#x00448;&#x00435;&#x0043d;&#x00441;&#x00442;&#x00432;&#x0043e;&#x00432;&#x00430;&#x0043d;&#x00438;&#x0044f; &#x0043e;&#x00431;&#x00440;&#x00430;&#x00437;&#x00430; &#x00436;&#x00438;&#x00437;&#x0043d;&#x00438; &#x00432; &#x00422;&#x00430;&#x00438;&#x00