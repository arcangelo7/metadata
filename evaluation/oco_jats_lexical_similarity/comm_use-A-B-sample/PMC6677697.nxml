				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\begin{aligned} \partial _tp(\mathbf {y},t)= &#x00026; {} \displaystyle {\sum _{j=1}^N\sum _{r=1}^{R} w_r(\mathbf {y}_{\cdot j}- \mathbf n_r, t)p(\mathbf {y}_{\cdot 1}, \mathbf {y}_{\cdot 2},\ldots , \mathbf {y}_{\cdot j}- \mathbf n_r,\ldots , \mathbf {y}_{\cdot N}, t)} \nonumber \\&#x00026;\displaystyle {-\sum _{j=1}^N\sum _{r=1}^{R} w_r(\mathbf {y}_{\cdot j}, t)p(\mathbf {y}, t)} \equiv \mathcal {M}p(\mathbf {y}, t), \end{aligned}$$\end{document}</tex-math><mml:math id="M108" display="block"><mml:mrow><mml:mtable><mml:mtr><mml:mtd columnalign="right"><mml:mrow><mml:msub><mml:mi>&#x02202;</mml:mi><mml:mi>t</mml:mi></mml:msub><mml:mi>p</mml:mi><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mi mathvariant="bold">y</mml:mi><mml:mo>,</mml:mo><mml:mi>t</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mo>=</mml:mo></mml:mrow></mml:mtd><mml:mtd columnalign="left"><mml:mstyle displaystyle="true" scriptlevel="0"><mml:mrow><mml:mrow/><mml:mrow><mml:munderover><mml:mo>&#x02211;</mml:mo><mml:mrow><mml:mi>j</mml:mi><mml:mo>=</mml:mo><mml:mn>1</mml:mn></mml:mrow><mml:mi>N</mml:mi></mml:munderover><mml:munderover><mml:mo>&#x02211;</mml:mo><mml:mrow><mml:mi>r</mml:mi><mml:mo>=</mml:mo><mml:mn>1</mml:mn></mml:mrow><mml:mi>R</mml:mi></mml:munderover><mml:msub><mml:mi>w</mml:mi><mml:mi>r</mml:mi></mml:msub><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:msub><mml:mi mathvariant="bold">y</mml:mi><mml:mrow><mml:mo>&#x000b7;</mml:mo><mml:mi>j</mml:mi></mml:mrow></mml:msub><mml:mo>-</mml:mo><mml:msub><mml:mi mathvariant="bold">n</mml:mi><mml:mi>r</mml:mi></mml:msub><mml:mo>,</mml:mo><mml:mi>t</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mi>p</mml:mi><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:msub><mml:mi mathvariant="bold">y</mml:mi><mml:mrow><mml:mo>&#x000b7;</mml:mo><mml:mn>1</mml:mn></mml:mrow></mml:msub><mml:mo>,</mml:mo><mml:msub><mml:mi mathvariant="bold">y</mml:mi><mml:mrow><mml:mo>&#x000b7;</mml:mo><mml:mn>2</mml:mn></mml: