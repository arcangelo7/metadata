				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\begin{aligned} I_{1j}(\sigma )= &#x00026; {} \displaystyle {\int _\mathbb R\exp \left( -(\xi _{1j}-\zeta )^2/\tau -\zeta ^2/\sigma ^2\right) \, \mathrm{d}\zeta }\nonumber \\= &#x00026; {} \displaystyle {\exp \left( -\xi _{1j}^2/\tau +\xi _{1j}^2/\alpha \tau ^2\right) \int _\mathbb R\exp \left( -\alpha (\zeta -\xi _{1j}/\alpha \tau )^2\right) \, \mathrm{d}\zeta }\nonumber \\= &#x00026; {} \displaystyle {\frac{\exp \left( -\xi _{1j}^2/(\tau +\sigma ^2)\right) }{\sqrt{\alpha }}\int _\mathbb R\exp (-z^2)\, \mathrm{d}z}\nonumber \\= &#x00026; {} \displaystyle {\frac{\sigma \sqrt{\pi \tau }}{\sqrt{\tau +\sigma ^2}} \exp \left( -\xi _{1j}^2/(\tau +\sigma ^2)\right) }. \end{aligned}$$\end{document}</tex-math><mml:math id="M674" display="block"><mml:mrow><mml:mtable><mml:mtr><mml:mtd columnalign="right"><mml:mrow><mml:msub><mml:mi>I</mml:mi><mml:mrow><mml:mn>1</mml:mn><mml:mi>j</mml:mi></mml:mrow></mml:msub><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mi>&#x003c3;</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mo>=</mml:mo></mml:mrow></mml:mtd><mml:mtd columnalign="left"><mml:mstyle displaystyle="true" scriptlevel="0"><mml:mrow><mml:mrow/><mml:mrow><mml:msub><mml:mo>&#x0222b;</mml:mo><mml:mi mathvariant="double-struck">R</mml:mi></mml:msub><mml:mo>exp</mml:mo><mml:mfenced close=")" open="("><mml:mo>-</mml:mo><mml:msup><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:msub><mml:mi>&#x003be;</mml:mi><mml:mrow><mml:mn>1</mml:mn><mml:mi>j</mml:mi></mml:mrow></mml:msub><mml:mo>-</mml:mo><mml:mi>&#x003b6;</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mn>2</mml:mn></mml:msup><mml:mo stretchy="false">/</mml:mo><mml:mi>&#x003c4;</mml:mi><mml:mo>-</mml:mo><mml:msup><mml:mi>&#x003b6;</mml:mi><mml:mn>2</mml:mn></mml:msup><mml:mo stretchy="false">/</mml:mo><mml:msup><mml:mi>&#x003c3;</mml:mi><mml:mn>2</mml:mn></mml:msup></mml:mfenced><mml:mspace width="0.166667em"/><mml:mi mathvariant="normal">d</mml:mi><mml:mi>&#x003b6;</mml:mi></mml: