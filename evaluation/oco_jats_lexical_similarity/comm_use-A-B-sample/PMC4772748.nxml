<article xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Bone Res</journal-id><journal-id journal-id-type="iso-abbrev">Bone Res</journal-id><journal-title-group><journal-title>Bone Research</journal-title></journal-title-group><issn pub-type="ppub">2095-4700</issn><issn pub-type="epub">2095-6231</issn><publisher><publisher-name>Nature Publishing Group</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">26962465</article-id><article-id pub-id-type="pmc">4772748</article-id><article-id pub-id-type="pii">boneres201542</article-id><article-id pub-id-type="doi">10.1038/boneres.2015.42</article-id><article-categories><subj-group subj-group-type="heading"><subject>Article</subject></subj-group></article-categories><title-group><article-title>Characterization of genetically engineered mouse models carrying Col2a1-cre-induced deletions of Lrp5 and/or Lrp6</article-title><alt-title alt-title-type="running">Genetically engineered mouse models</alt-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Schumacher</surname><given-names>Cassie A</given-names></name><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="author-notes" rid="note1">&#x02217;</xref></contrib><contrib contrib-type="author"><name><surname>Joiner</surname><given-names>Danese M</given-names></name><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="author-notes" rid="note1">&#x02217;</xref></contrib><contrib contrib-type="author"><name><surname>Less</surname><given-names>Kennen D</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Drewry</surname><given-names>Melissa Oosterhouse</given-names></name><xref ref-type="aff" rid="aff1">1</xref></contrib><contrib contrib-type="author"><name><surname>Williams</surname><given-names>Bart O</given-names></name><xref ref-type="aff" rid="aff1">1</xref><xref ref-type="corresp" rid="caf1">*</xref></contrib><aff id="aff1"><label>1</label>
