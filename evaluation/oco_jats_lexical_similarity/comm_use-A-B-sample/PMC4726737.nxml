<mml:mfenced close=")" open="(" separators=""><mml:mfrac><mml:mi>r</mml:mi><mml:mrow><mml:mn>2</mml:mn><mml:mi mathvariant="italic">&#x003bb;</mml:mi><mml:msup><mml:mi>b</mml:mi><mml:mn>2</mml:mn></mml:msup></mml:mrow></mml:mfrac></mml:mfenced><mml:msup><mml:mrow><mml:mi mathvariant="normal">e</mml:mi></mml:mrow><mml:mrow><mml:mi>x</mml:mi><mml:mo stretchy="false">/</mml:mo><mml:mi>b</mml:mi></mml:mrow></mml:msup><mml:msubsup><mml:mo>&#x0222b;</mml:mo><mml:mrow><mml:mi>x</mml:mi></mml:mrow><mml:mrow><mml:mi>L</mml:mi><mml:mo stretchy="false">/</mml:mo><mml:mn>2</mml:mn></mml:mrow></mml:msubsup><mml:mrow><mml:msup><mml:mrow><mml:mi mathvariant="normal">e</mml:mi></mml:mrow><mml:mrow><mml:mo>-</mml:mo><mml:mi>y</mml:mi><mml:mo stretchy="false">/</mml:mo><mml:mi>b</mml:mi></mml:mrow></mml:msup><mml:msub><mml:mi mathvariant="italic">&#x003d5;</mml:mi><mml:mn>2</mml:mn></mml:msub><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mi>y</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mi mathvariant="normal">d</mml:mi><mml:mi>y</mml:mi></mml:mrow></mml:mrow></mml:mtd></mml:mtr></mml:mtable></mml:mrow></mml:math><graphic xlink:href="11538_2015_129_Article_Equ92.gif" position="anchor"/></alternatives></disp-formula><disp-formula id="Equ93"><label>88b</label><alternatives><tex-math id="M817">\documentclass[12pt]{minimal}
