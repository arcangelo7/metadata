				\begin{document}$$\begin{aligned} f_m= &#x00026; {} \frac{-g_R[R_T]Ne^F\varPhi ^2-g_B[B_T]^2b_p^2Ne^F\varPhi ^2}{2k_5},\\ f_{b_p}= &#x00026; {} \frac{-2g_B[B_T]^2b_p\varPhi }{k_5},\\ g_m= &#x00026; {} \frac{k_1(1-a_p)Ne^F\varPhi ^2}{2k_5},\\ g_{a_p}= &#x00026; {} \frac{-1}{k_5}\left( k_1\varPhi +k_2[Y_T](1-y_p^*)+k_2[Y_T]\left( y_p^{*2}-y_p^*\right) +k_3[B_T](1-b_p)\right) ,\\ g_{b_p}= &#x00026; {} \frac{k_3[B_T]a_p}{k_5},\\ h_{a_p}= &#x00026; {} \frac{k_3[A_T](1-b_p)}{k_5},\\ h_{b_p}= &#x00026; {} \frac{-k_3[A_T]a_p}{k_5}-1 \end{aligned}$$\end{document}</tex-math><mml:math id="M234" display="block"><mml:mrow><mml:mtable><mml:mtr><mml:mtd columnalign="right"><mml:mrow><mml:msub><mml:mi>f</mml:mi><mml:mi>m</mml:mi></mml:msub><mml:mo>=</mml:mo></mml:mrow></mml:mtd><mml:mtd columnalign="left"><mml:mrow><mml:mrow/><mml:mfrac><mml:mrow><mml:mo>-</mml:mo><mml:msub><mml:mi>g</mml:mi><mml:mi>R</mml:mi></mml:msub><mml:mrow><mml:mo stretchy="false">[</mml:mo><mml:msub><mml:mi>R</mml:mi><mml:mi>T</mml:mi></mml:msub><mml:mo stretchy="false">]</mml:mo></mml:mrow><mml:mi>N</mml:mi><mml:msup><mml:mi>e</mml:mi><mml:mi>F</mml:mi></mml:msup><mml:msup><mml:mi>&#x003a6;</mml:mi><mml:mn>2</mml:mn></mml:msup><mml:mo>-</mml:mo><mml:msub><mml:mi>g</mml:mi><mml:mi>B</mml:mi></mml:msub><mml:msup><mml:mrow><mml:mo stretchy="false">[</mml:mo><mml:msub><mml:mi>B</mml:mi><mml:mi>T</mml:mi></mml:msub><mml:mo stretchy="false">]</mml:mo></mml:mrow><mml:mn>2</mml:mn></mml:msup><mml:msubsup><mml:mi>b</mml:mi><mml:mi>p</mml:mi><mml:mn>2</mml:mn></mml:msubsup><mml:mi>N</mml:mi><mml:msup><mml:mi>e</mml:mi><mml:mi>F</mml:mi></mml:msup><mml:msup><mml:mi>&#x003a6;</mml:mi><mml:mn>2</mml:mn></mml:msup></mml:mrow><mml:mrow><mml:mn>2</mml:mn><mml:msub><mml:mi>k</mml:mi><mml:mn>5</mml:mn></mml:msub></mml:mrow></mml:mfrac><mml:mo>,</mml:mo></mml:mrow></mml:mtd></mml:mtr><mml:mtr><mml:mtd columnalign="right"><mml:mrow><mml:mrow/><mml:msub><mml:mi>f</mml:mi><mml:msub><mml:mi>b</mml:mi><mml:mi>p</mml:mi></mml: