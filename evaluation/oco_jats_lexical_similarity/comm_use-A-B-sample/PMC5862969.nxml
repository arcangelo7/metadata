				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\begin{aligned} \frac{\partial f_1}{\partial m}= &#x00026; {} \frac{-Ne^F\varPhi ^2}{2k_5}\left( g_R[R_T]+g_B[B_T]^2b_p^2\right) ,\\ \frac{\partial f_1}{\partial b_p}= &#x00026; {} \frac{-2g_B[B_T]^2b_p\varPhi }{k_5},\\ \frac{\partial f_2}{\partial m}= &#x00026; {} \frac{Nk_1(1-a_p)e^F\varPhi }{2k_5},\\ \frac{\partial f_2}{\partial a_p}= &#x00026; {} \frac{k_1\varPhi }{k_5}-\frac{k_2[Y_T](1-y_p)}{k_5}-\frac{k_3[B_T](1-b_p)}{k_5},\\ \frac{\partial f_2}{\partial y_p}= &#x00026; {} \frac{k_2[Y_T]a_p}{k_5},\\ \frac{\partial f_2}{\partial b_p}= &#x00026; {} \frac{k_3[B_T]a_p}{k_5},\\ \frac{\partial f_3}{\partial a_p}= &#x00026; {} \frac{k_2[A_T](1-y_p)}{k5},\\ \frac{\partial f_3}{\partial y_p}= &#x00026; {} \frac{-k_2[A_T]a_p}{k_5}-\frac{k_4[Z_T]}{k_5}-\frac{k_6}{k_5},\\ \frac{\partial f_4}{\partial a_p}= &#x00026; {} \frac{k_3[A_T](1-b_p)}{k_5},\\ \frac{\partial f_4}{\partial b_p}= &#x00026; {} \frac{-k_3[A_T]a_p}{k_5}-1. \end{aligned}$$\end{document}</tex-math><mml:math id="M184" display="block"><mml:mrow><mml:mtable><mml:mtr><mml:mtd columnalign="right"><mml:mrow><mml:mfrac><mml:mrow><mml:mi>&#x02202;</mml:mi><mml:msub><mml:mi>f</mml:mi><mml:mn>1</mml:mn></mml:msub></mml:mrow><mml:mrow><mml:mi>&#x02202;</mml:mi><mml:mi>m</mml:mi></mml:mrow></mml:mfrac><mml:mo>=</mml:mo></mml:mrow></mml:mtd><mml:mtd columnalign="left"><mml:mrow><mml:mrow/><mml:mfrac><mml:mrow><mml:mo>-</mml:mo><mml:mi>N</mml:mi><mml:msup><mml:mi>e</mml:mi><mml:mi>F</mml:mi></mml:msup><mml:msup><mml:mi>&#x003a6;</mml:mi><mml:mn>2</mml:mn></mml:msup></mml:mrow><mml:mrow><mml:mn>2</mml:mn><mml:msub><mml:mi>k</mml:mi><mml:mn>5</mml:mn></mml:msub></mml:mrow></mml:mfrac><mml:mfenced close=")" open="(" separators=""><mml:msub><mml:mi>g</mml:mi><mml:mi>R</mml:mi></mml:msub><mml:mrow><mml:mo stretchy="false">[</mml:mo><mml:msub><mml:mi>R</mml:mi><mml:mi>T</mml:mi></mml:msub><mml:mo stretchy="false">]</mml:mo></mml:mrow><mml:mo>+</mml:mo><mml:msub><mml:mi>g</mml:mi><mml:mi>B</mml:mi></mml:m