</mml:mi><mml:mo>&#x000b1;</mml:mo></mml:msup></mml:mrow><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mi>p</mml:mi><mml:mo>+</mml:mo><mml:mn>1</mml:mn><mml:mo stretchy="false">)</mml:mo></mml:mrow></mml:msubsup><mml:mo>=</mml:mo><mml:msubsup><mml:mi>y</mml:mi><mml:mrow><mml:msup><mml:mi>i</mml:mi><mml:mo>&#x000b1;</mml:mo></mml:msup></mml:mrow><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mi>p</mml:mi><mml:mo>+</mml:mo><mml:mn>1</mml:mn><mml:mo stretchy="false">)</mml:mo></mml:mrow></mml:msubsup><mml:mo>&#x000b1;</mml:mo><mml:mi mathvariant="italic">&#x003b2;</mml:mi><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:msub><mml:mi mathvariant="italic">&#x003bb;</mml:mi><mml:mn>1</mml:mn></mml:msub><mml:mo>&#x000b1;</mml:mo><mml:msub><mml:mi mathvariant="italic">&#x003bb;</mml:mi><mml:mn>2</mml:mn></mml:msub><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mo>cos</mml:mo><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:msup><mml:mi mathvariant="italic">&#x003c9;</mml:mi><mml:mo>&#x000b1;</mml:mo></mml:msup><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mo>,</mml:mo></mml:mrow></mml:mtd></mml:mtr></mml:mtable></mml:mrow></mml:mfenced></mml:mtd></mml:mtr></mml:mtable></mml:mrow></mml:math><graphic xlink:href="11538_2017_333_Article_Equ15.gif" position="anchor"/></alternatives></disp-formula>where <inline-formula id="IEq131"><alternatives><tex-math id="M291">\documentclass[12pt]{minimal}
