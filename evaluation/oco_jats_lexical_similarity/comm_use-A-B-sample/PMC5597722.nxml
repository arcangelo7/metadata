				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\begin{aligned} \left( u_{1}^{*},u_{2}^{*},\varphi _{1}^{*},\varphi _{2}^{*}\right) =\left( 0,\frac{A_{2}}{b_{2}},A_{1}-b_{1}\frac{A_{2}}{b_{2} },0\right) , \end{aligned}$$\end{document}</tex-math><mml:math id="M198" display="block"><mml:mrow><mml:mtable><mml:mtr><mml:mtd columnalign="right"><mml:mrow><mml:mfenced close=")" open="(" separators=""><mml:msubsup><mml:mi>u</mml:mi><mml:mrow><mml:mn>1</mml:mn></mml:mrow><mml:mrow><mml:mrow/><mml:mo>&#x02217;</mml:mo></mml:mrow></mml:msubsup><mml:mo>,</mml:mo><mml:msubsup><mml:mi>u</mml:mi><mml:mrow><mml:mn>2</mml:mn></mml:mrow><mml:mrow><mml:mrow/><mml:mo>&#x02217;</mml:mo></mml:mrow></mml:msubsup><mml:mo>,</mml:mo><mml:msubsup><mml:mi mathvariant="italic">&#x003c6;</mml:mi><mml:mrow><mml:mn>1</mml:mn></mml:mrow><mml:mrow><mml:mrow/><mml:mo>&#x02217;</mml:mo></mml:mrow></mml:msubsup><mml:mo>,</mml:mo><mml:msubsup><mml:mi mathvariant="italic">&#x003c6;</mml:mi><mml:mrow><mml:mn>2</mml:mn></mml:mrow><mml:mrow><mml:mrow/><mml:mo>&#x02217;</mml:mo></mml:mrow></mml:msubsup></mml:mfenced><mml:mo>=</mml:mo><mml:mfenced close=")" open="(" separators=""><mml:mn>0</mml:mn><mml:mo>,</mml:mo><mml:mfrac><mml:msub><mml:mi>A</mml:mi><mml:mn>2</mml:mn></mml:msub><mml:msub><mml:mi>b</mml:mi><mml:mn>2</mml:mn></mml:msub></mml:mfrac><mml:mo>,</mml:mo><mml:msub><mml:mi>A</mml:mi><mml:mn>1</mml:mn></mml:msub><mml:mo>-</mml:mo><mml:msub><mml:mi>b</mml:mi><mml:mn>1</mml:mn></mml:msub><mml:mfrac><mml:msub><mml:mi>A</mml:mi><mml:mn>2</mml:mn></mml:msub><mml:msub><mml:mi>b</mml:mi><mml:mn>2</mml:mn></mml:msub></mml:mfrac><mml:mo>,</mml:mo><mml:mn>0</mml:mn></mml:mfenced><mml:mo>,</mml:mo></mml:mrow></mml:mtd></mml:mtr></mml:mtable></mml:mrow></mml:math><graphic xlink:href="11538_2017_331_Article_Equ15.gif" position="anchor"/></alternatives></disp-formula>
<disp-formula id="Equ16"><label>16</label><alternatives><tex-math id="M199">\documentclass[12pt]{minimal}
