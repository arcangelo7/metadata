				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\begin{aligned} \eta = \left( A_1 - \frac{b_1}{b_2}A_2 \right) (r_1+k^2) - \delta _1 k^2 \end{aligned}$$\end{document}</tex-math><mml:math id="M400" display="block"><mml:mrow><mml:mtable><mml:mtr><mml:mtd columnalign="right"><mml:mrow><mml:mi mathvariant="italic">&#x003b7;</mml:mi><mml:mo>=</mml:mo><mml:mfenced close=")" open="(" separators=""><mml:msub><mml:mi>A</mml:mi><mml:mn>1</mml:mn></mml:msub><mml:mo>-</mml:mo><mml:mfrac><mml:msub><mml:mi>b</mml:mi><mml:mn>1</mml:mn></mml:msub><mml:msub><mml:mi>b</mml:mi><mml:mn>2</mml:mn></mml:msub></mml:mfrac><mml:msub><mml:mi>A</mml:mi><mml:mn>2</mml:mn></mml:msub></mml:mfenced><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:msub><mml:mi>r</mml:mi><mml:mn>1</mml:mn></mml:msub><mml:mo>+</mml:mo><mml:msup><mml:mi>k</mml:mi><mml:mn>2</mml:mn></mml:msup><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mo>-</mml:mo><mml:msub><mml:mi mathvariant="italic">&#x003b4;</mml:mi><mml:mn>1</mml:mn></mml:msub><mml:msup><mml:mi>k</mml:mi><mml:mn>2</mml:mn></mml:msup></mml:mrow></mml:mtd></mml:mtr></mml:mtable></mml:mrow></mml:math><graphic xlink:href="11538_2017_331_Article_Equ27.gif" position="anchor"/></alternatives></disp-formula>or<disp-formula id="Equ28"><label>28</label><alternatives><tex-math id="M401">\documentclass[12pt]{minimal}
