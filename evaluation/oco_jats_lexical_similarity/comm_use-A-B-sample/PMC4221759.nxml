0;, &#x00438; &#x00443; 28 (4,8%) &#x00438;&#x00437; 581 &#x00438;&#x00437;&#x0043e;&#x0043b;&#x0044f;&#x00442;&#x0043e;&#x00432;, &#x00432;&#x00437;&#x0044f;&#x00442;&#x0044b;&#x00445; &#x00443; &#x0043b;&#x00438;&#x00446;, &#x0043f;&#x0043e;&#x0043b;&#x00443;&#x00447;&#x00430;&#x00432;&#x00448;&#x00438;&#x00445; &#x0043f;&#x0043e;&#x00432;&#x00442;&#x0043e;&#x00440;&#x0043d;&#x0043e;&#x00435; &#x0043b;&#x00435;&#x00447;&#x00435;&#x0043d;&#x00438;&#x00435;. &#x00418;&#x00437; &#x00438;&#x00437;&#x0043e;&#x0043b;&#x0044f;&#x00442;&#x0043e;&#x00432;, &#x00432;&#x00437;&#x0044f;&#x00442;&#x0044b;&#x00445; &#x00443; &#x0043b;&#x00438;&#x00446;, &#x0043f;&#x0043e;&#x0043b;&#x00443;&#x00447;&#x00430;&#x00432;&#x00448;&#x00438;&#x00445; &#x0043f;&#x0043e;&#x00432;&#x00442;&#x0043e;&#x00440;&#x0043d;&#x0043e;&#x00435; &#x0043b;&#x00435;&#x00447;&#x00435;&#x0043d;&#x00438;&#x00435; &#x0043f;&#x0043e;&#x00441;&#x0043b;&#x00435; &#x0043d;&#x00435;&#x00443;&#x00434;&#x00430;&#x00447;&#x0043d;&#x0043e;&#x00433;&#x0043e; &#x0043f;&#x00435;&#x00440;&#x00432;&#x00438;&#x00447;&#x0043d;&#x0043e;&#x00433;&#x0043e;, &#x0043c;&#x0043d;&#x0043e;&#x00436;&#x00435;&#x00441;&#x00442;&#x00432;&#x00435;&#x0043d;&#x0043d;&#x00430;&#x0044f; &#x0043b;&#x00435;&#x0043a;&#x00430;&#x00440;&#x00441;&#x00442;&#x00432;&#x00435;&#x0043d;&#x0043d;&#x00430;&#x0044f; &#x00443;&#x00441;&#x00442;&#x0043e;&#x00439;&#x00447;&#x00438;&#x00432;&#x0043e;&#x00441;&#x00442;&#x0044c; &#x00431;&#x0044b;&#x0043b;&#x00430; &#x00432;&#x0044b;&#x0044f;&#x00432;&#x0043b;&#x00435;&#x0043d;&#x00430; &#x00432; &#x00434;&#x00435;&#x00432;&#x0044f;&#x00442;&#x00438; (29%) &#x00441;&#x0043b;&#x00443;&#x00447;&#x00430;&#x0044f;&#x00445; &#x00438;&#x00437; 31. &#x0041f;&#x00440;&#x00438;&#x00442;&#x0043e;&#x0043c;, &#x00447;&#x00442;&#x0043e; &#x00443;&#x00441;&#x00442;&#x0043e;&#x00439;&#x00447;&#x00438;&#x00432;&#x0043e;&#x00441;&#x00442;&#x0044c; &#x0043a; &#x0043b;&#x00435;&#x0043a;&#x00430;&#x00440;&#x00441;&#x00442;&#x00