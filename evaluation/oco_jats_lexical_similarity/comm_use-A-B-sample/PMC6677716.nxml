				\begin{document}$$\begin{aligned} W_{dwSSA}(\mathbf {J})= &#x00026; {} \prod _{i=1}^{N_{{\mathscr {T}}}} \left[ \frac{a_{j'_i}(\mathbf {X}(t_i)) e^{-a_0(\mathbf {X}(t_i)) \tau _i}}{b_{j'_i}(\mathbf {X}(t_i)) e^{-b_0(\mathbf {X}(t_i)) \tau _i}} \right] \nonumber \\= &#x00026; {} \prod _{i=1}^{N_{{\mathscr {T}}}} \left[ \exp { \left\{ \left( b_0(\mathbf {X}(t_i)) - a_0(\mathbf {X}(t_i)) \right) \tau _i \right\} } \times (\gamma _{j'_i})^{-1} \right] . \end{aligned}$$\end{document}</tex-math><mml:math id="M122" display="block"><mml:mrow><mml:mtable><mml:mtr><mml:mtd columnalign="right"><mml:mrow><mml:msub><mml:mi>W</mml:mi><mml:mrow><mml:mi mathvariant="italic">dwSSA</mml:mi></mml:mrow></mml:msub><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mi mathvariant="bold">J</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mo>=</mml:mo></mml:mrow></mml:mtd><mml:mtd columnalign="left"><mml:mrow><mml:mrow/><mml:munderover><mml:mo>&#x0220f;</mml:mo><mml:mrow><mml:mi>i</mml:mi><mml:mo>=</mml:mo><mml:mn>1</mml:mn></mml:mrow><mml:msub><mml:mi>N</mml:mi><mml:mi mathvariant="script">T</mml:mi></mml:msub></mml:munderover><mml:mfenced close="]" open="["><mml:mfrac><mml:mrow><mml:msub><mml:mi>a</mml:mi><mml:msubsup><mml:mi>j</mml:mi><mml:mi>i</mml:mi><mml:mo>&#x02032;</mml:mo></mml:msubsup></mml:msub><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mi mathvariant="bold">X</mml:mi><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:msub><mml:mi>t</mml:mi><mml:mi>i</mml:mi></mml:msub><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:msup><mml:mi>e</mml:mi><mml:mrow><mml:mo>-</mml:mo><mml:msub><mml:mi>a</mml:mi><mml:mn>0</mml:mn></mml:msub><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mi mathvariant="bold">X</mml:mi><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:msub><mml:mi>t</mml:mi><mml:mi>i</mml:mi></mml:msub><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:msub><mml:mi>&