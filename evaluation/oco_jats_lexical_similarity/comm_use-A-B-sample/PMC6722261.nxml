				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\begin{aligned} \frac{\text {d}X}{\text {d}Y}= &#x00026; {} K_A10^Y\ln {10}=X\ln {10}, \end{aligned}$$\end{document}</tex-math><mml:math id="M378" display="block"><mml:mrow><mml:mtable><mml:mtr><mml:mtd columnalign="right"><mml:mrow><mml:mfrac><mml:mrow><mml:mtext>d</mml:mtext><mml:mi>X</mml:mi></mml:mrow><mml:mrow><mml:mtext>d</mml:mtext><mml:mi>Y</mml:mi></mml:mrow></mml:mfrac><mml:mo>=</mml:mo></mml:mrow></mml:mtd><mml:mtd columnalign="left"><mml:mrow><mml:mrow/><mml:msub><mml:mi>K</mml:mi><mml:mi>A</mml:mi></mml:msub><mml:msup><mml:mn>10</mml:mn><mml:mi>Y</mml:mi></mml:msup><mml:mo>ln</mml:mo><mml:mn>10</mml:mn><mml:mo>=</mml:mo><mml:mi>X</mml:mi><mml:mo>ln</mml:mo><mml:mn>10</mml:mn><mml:mo>,</mml:mo></mml:mrow></mml:mtd></mml:mtr></mml:mtable></mml:mrow></mml:math><graphic xlink:href="11538_2017_387_Article_Equ51.gif" position="anchor"/></alternatives></disp-formula><disp-formula id="Equ52"><label>40</label><alternatives><tex-math id="M379">\documentclass[12pt]{minimal}
