				\usepackage{amsfonts} 
				\usepackage{amssymb} 
				\usepackage{amsbsy}
				\usepackage{mathrsfs}
				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\begin{aligned} \frac{\text {d}A_\mathrm{bound}}{\text {d}X}= &#x00026; {} \frac{1+4\alpha X+\alpha X^2}{(1+X+\alpha X^2)^2}D_\mathrm{tot}, \end{aligned}$$\end{document}</tex-math><mml:math id="M382" display="block"><mml:mrow><mml:mtable><mml:mtr><mml:mtd columnalign="right"><mml:mrow><mml:mfrac><mml:mrow><mml:mtext>d</mml:mtext><mml:msub><mml:mi>A</mml:mi><mml:mi mathvariant="normal">bound</mml:mi></mml:msub></mml:mrow><mml:mrow><mml:mtext>d</mml:mtext><mml:mi>X</mml:mi></mml:mrow></mml:mfrac><mml:mo>=</mml:mo></mml:mrow></mml:mtd><mml:mtd columnalign="left"><mml:mrow><mml:mrow/><mml:mfrac><mml:mrow><mml:mn>1</mml:mn><mml:mo>+</mml:mo><mml:mn>4</mml:mn><mml:mi>&#x003b1;</mml:mi><mml:mi>X</mml:mi><mml:mo>+</mml:mo><mml:mi>&#x003b1;</mml:mi><mml:msup><mml:mi>X</mml:mi><mml:mn>2</mml:mn></mml:msup></mml:mrow><mml:msup><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mn>1</mml:mn><mml:mo>+</mml:mo><mml:mi>X</mml:mi><mml:mo>+</mml:mo><mml:mi>&#x003b1;</mml:mi><mml:msup><mml:mi>X</mml:mi><mml:mn>2</mml:mn></mml:msup><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mn>2</mml:mn></mml:msup></mml:mfrac><mml:msub><mml:mi>D</mml:mi><mml:mi mathvariant="normal">tot</mml:mi></mml:msub><mml:mo>,</mml:mo></mml:mrow></mml:mtd></mml:mtr></mml:mtable></mml:mrow></mml:math><graphic xlink:href="11538_2017_387_Article_Equ53.gif" position="anchor"/></alternatives></disp-formula><disp-formula id="Equ54"><label>42</label><alternatives><tex-math id="M383">\documentclass[12pt]{minimal}
				\usepackage{amsmath}
