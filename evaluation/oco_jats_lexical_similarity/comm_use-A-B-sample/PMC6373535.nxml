				\begin{document}$$\begin{aligned} \frac{\mathrm{d}}{\mathrm{d}t}\int _{\hat{\varOmega }(t)} u \, \mathrm{d}\varOmega = \int _{\hat{\varOmega }(t)} \left( \frac{\partial u}{\partial t} + \nabla _{\varOmega (t)}\cdot (\mathbf {Q}u) \right) \mathrm{d}\varOmega , \end{aligned}$$\end{document}</tex-math><mml:math id="M66" display="block"><mml:mrow><mml:mtable><mml:mtr><mml:mtd columnalign="right"><mml:mrow><mml:mfrac><mml:mi mathvariant="normal">d</mml:mi><mml:mrow><mml:mi mathvariant="normal">d</mml:mi><mml:mi>t</mml:mi></mml:mrow></mml:mfrac><mml:msub><mml:mo>&#x0222b;</mml:mo><mml:mrow><mml:mover accent="true"><mml:mi>&#x003a9;</mml:mi><mml:mo stretchy="false">^</mml:mo></mml:mover><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mi>t</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow></mml:mrow></mml:msub><mml:mi>u</mml:mi><mml:mspace width="0.166667em"/><mml:mi mathvariant="normal">d</mml:mi><mml:mi>&#x003a9;</mml:mi><mml:mo>=</mml:mo><mml:msub><mml:mo>&#x0222b;</mml:mo><mml:mrow><mml:mover accent="true"><mml:mi>&#x003a9;</mml:mi><mml:mo stretchy="false">^</mml:mo></mml:mover><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mi>t</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow></mml:mrow></mml:msub><mml:mfenced close=")" open="("><mml:mfrac><mml:mrow><mml:mi>&#x02202;</mml:mi><mml:mi>u</mml:mi></mml:mrow><mml:mrow><mml:mi>&#x02202;</mml:mi><mml:mi>t</mml:mi></mml:mrow></mml:mfrac><mml:mo>+</mml:mo><mml:msub><mml:mi mathvariant="normal">&#x02207;</mml:mi><mml:mrow><mml:mi>&#x003a9;</mml:mi><mml:mo stretchy="false">(</mml:mo><mml:mi>t</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow></mml:msub><mml:mo>&#x000b7;</mml:mo><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mi mathvariant="bold">Q</mml:mi><mml:mi>u</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow></mml:mfenced><mml:mi mathvariant="normal">d</mml:mi><mml:mi>&#x003a9;</mml:mi><mml:mo>,</mml:mo></mml:mrow></mml:mtd></mml:mtr></mml:mtable></mml:mrow></mml:math><graphic xlink:href="11538_2018_535_Article_Equ9.gif" position="anchor"/></alternatives></disp-formula>where <inline-formula id="IEq25"><alternatives><tex-math id="M67">\documentclass[12pt]{minimal}
