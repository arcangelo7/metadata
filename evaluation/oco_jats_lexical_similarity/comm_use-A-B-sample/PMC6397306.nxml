				\begin{document}$$\begin{aligned} S^{2}= &#x00026; {} \left( \frac{b_{1}-b_{1}^{ref}}{b_{1}^{ref}}\right) ^{2} +\left( \frac{b_{2}-b_{2}^{ref}}{b_{2}^{ref}}\right) ^{2} +\left( \frac{b_{3}-b_{3}^{ref}}{b_{3}^{ref}}\right) ^{2} +\left( \frac{b_{4}-b_{4}^{ref}}{b_{4}^{ref}}\right) ^{2}\nonumber \\&#x00026;+\,\left( \frac{b_{5}-b_{5}^{ref}}{b_{5}^{ref}}\right) ^{2} +\left( \frac{a_{1,M2}-a_{1,M2}^{ref}}{a_{1,M2}^{ref}} \right) ^{2}+\left( \frac{a_{1,VC}-a_{1,VC}^{ref}}{a_{1,VC}^{ref}}\right) ^{2}\nonumber \\&#x00026;+\,\left( \frac{a_{2,M2}-a_{2,M2}^{ref}}{a_{2,M2}^{ref}} \right) ^{2}+\left( \frac{a_{2,VC}-a_{2,VC}^{ref}}{a_{2,VC}^{ref}}\right) ^{2}. \end{aligned}$$\end{document}</tex-math><mml:math id="M618" display="block"><mml:mrow><mml:mtable><mml:mtr><mml:mtd columnalign="right"><mml:mrow><mml:msup><mml:mi>S</mml:mi><mml:mn>2</mml:mn></mml:msup><mml:mo>=</mml:mo></mml:mrow></mml:mtd><mml:mtd columnalign="left"><mml:mrow><mml:mrow/><mml:msup><mml:mfenced close=")" open="("><mml:mfrac><mml:mrow><mml:msub><mml:mi>b</mml:mi><mml:mn>1</mml:mn></mml:msub><mml:mo>-</mml:mo><mml:msubsup><mml:mi>b</mml:mi><mml:mrow><mml:mn>1</mml:mn></mml:mrow><mml:mrow><mml:mi mathvariant="italic">ref</mml:mi></mml:mrow></mml:msubsup></mml:mrow><mml:msubsup><mml:mi>b</mml:mi><mml:mrow><mml:mn>1</mml:mn></mml:mrow><mml:mrow><mml:mi mathvariant="italic">ref</mml:mi></mml:mrow></mml:msubsup></mml:mfrac></mml:mfenced><mml:mn>2</mml:mn></mml:msup><mml:mo>+</mml:mo><mml:msup><mml:mfenced close=")" open="("><mml:mfrac><mml:mrow><mml:msub><mml:mi>b</mml:mi><mml:mn>2</mml:mn></mml:msub><mml:mo>-</mml:mo><mml:msubsup><mml:mi>b</mml:mi><mml:mrow><mml:mn>2</mml:mn></mml:mrow><mml:mrow><mml:mi mathvariant="italic">ref</mml:mi></mml:mrow></mml:msubsup></mml:mrow><mml:msubsup><mml:mi>b</mml:mi><mml:mrow><mml:mn>2</mml:mn></mml:mrow><mml:mrow><mml:mi mathvariant="italic">ref</mml:mi></mml:mrow></mml:msubsup></mml:mfrac></mml:mfenced><mml:mn>2</mml:mn></mml:msup><mml:mo>+</mml:mo><mml:msup><mml:mfence