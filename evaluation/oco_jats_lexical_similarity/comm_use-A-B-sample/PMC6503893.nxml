				\usepackage{upgreek}
				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\begin{aligned} m^{n+1}_{i,j}&#x00026;= D_{m} \frac{\varDelta t_a }{(\varDelta x_a)^2}\left( m^{n}_{i+1,j}+m^{n}_{i-1,j}+m^{n}_{i,j+1}+m^{n}_{i,j-1} \right) \nonumber \\&#x00026;\ \ \ + m^{n}_{i,j}\left( 1 - 4 D_{m} \frac{\varDelta t_a}{(\varDelta x_a)^2}-\varDelta t \varLambda \right) + \varDelta t_a \varTheta {c_{_{\mathsf {M}}}}^{n}_{i,j} \nonumber \\ w^{n+1}_{i,j}&#x00026;= w^{n}_{i,j}\left[ 1- \varDelta t_a \left( \varGamma _{_1} {c_{_{\mathsf {M}}}}^{n}_{i,j}+ \varGamma _{_2} m^{n}_{i,j} \right) \right] \end{aligned}$$\end{document}</tex-math><mml:math id="M562" display="block"><mml:mrow><mml:mtable><mml:mtr><mml:mtd columnalign="right"><mml:msubsup><mml:mi>m</mml:mi><mml:mrow><mml:mi>i</mml:mi><mml:mo>,</mml:mo><mml:mi>j</mml:mi></mml:mrow><mml:mrow><mml:mi>n</mml:mi><mml:mo>+</mml:mo><mml:mn>1</mml:mn></mml:mrow></mml:msubsup></mml:mtd><mml:mtd columnalign="left"><mml:mrow><mml:mo>=</mml:mo><mml:msub><mml:mi>D</mml:mi><mml:mi>m</mml:mi></mml:msub><mml:mfrac><mml:mrow><mml:mi>&#x00394;</mml:mi><mml:msub><mml:mi>t</mml:mi><mml:mi>a</mml:mi></mml:msub></mml:mrow><mml:msup><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mi>&#x00394;</mml:mi><mml:msub><mml:mi>x</mml:mi><mml:mi>a</mml:mi></mml:msub><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mn>2</mml:mn></mml:msup></mml:mfrac><mml:mfenced close=")" open="("><mml:msubsup><mml:mi>m</mml:mi><mml:mrow><mml:mi>i</mml:mi><mml:mo>+</mml:mo><mml:mn>1</mml:mn><mml:mo>,</mml:mo><mml:mi>j</mml:mi></mml:mrow><mml:mi>n</mml:mi></mml:msubsup><mml:mo>+</mml:mo><mml:msubsup><mml:mi>m</mml:mi><mml:mrow><mml:mi>i</mml:mi><mml:mo>-</mml:mo><mml:mn>1</mml:mn><mml:mo>,</mml:mo><mml:mi>j</mml:mi></mml:mrow><mml:mi>n</mml:mi></mml:msubsup><mml:mo>+</mml:mo><mml:msubsup><mml:mi>m</mml:mi><mml:mrow><mml:mi>i</mml:mi><mml:mo>,</mml:mo><mml:mi>j</mml:mi><mml:mo>+</mml:mo><mml:mn>1</mml:mn></mml:mrow><mml:mi>n</mml:mi></mml:msubsup><mml:mo>+</mml:mo><mml:msubsup><mml:mi>m</mml:mi><mml:mrow><mml:mi>i</mml:mi><m