				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\begin{aligned} {\mathcal {P}}_0&#x00026;: {\mathcal {P}}^{n}_{i-1,j}:=\frac{ \varDelta t}{(\varDelta x)^2}\left[ D_{k} - \frac{\varPhi _{_k}}{4} \left( w^{n}_{i+1,j} -w^{n}_{i-1,j}\right) \right] ,\nonumber \\ {\mathcal {P}}_1&#x00026;: {\mathcal {P}}^{n}_{i+1,j}:=\frac{ \varDelta t}{(\varDelta x)^2}\left[ D_{k} + \frac{\varPhi _{_k}}{4} \left( w^{n}_{i+1,j} -w^{n}_{i-1,j}\right) \right] , \nonumber \\ {\mathcal {P}}_2&#x00026;: {\mathcal {P}}^{n}_{i,j+1}:=\frac{ \varDelta t}{(\varDelta x)^2}\left[ D_{k} + \frac{\varPhi _{_k}}{4} \left( w^{n}_{i,j+1} -w^{n}_{i,j-1}\right) \right] , \nonumber \\ {\mathcal {P}}_3&#x00026;: {\mathcal {P}}^{n}_{i,j-1}:=\frac{ \varDelta t}{(\varDelta x)^2}\left[ D_{k} - \frac{\varPhi _{_k}}{4} \left( w^{n}_{i,j+1} -w^{n}_{i,j-1}\right) \right] , \nonumber \\ {\mathcal {P}}_4&#x00026;: {\mathcal {P}}^{n}_{i,j}\ \ \ :=1-\frac{ \varDelta t}{(\varDelta x)^2}\left[ 4 D_{k} - \varPhi _{_k} \left( w^{n}_{i+1,j} +w^{n}_{i-1,j} + w^{n}_{i,j+1} +w^{n}_{i,j-1}- 4 w^{n}_{i,j}\right) \right] , \end{aligned}$$\end{document}</tex-math><mml:math id="M592" display="block"><mml:mrow><mml:mtable><mml:mtr><mml:mtd columnalign="right"><mml:msub><mml:mi mathvariant="script">P</mml:mi><mml:mn>0</mml:mn></mml:msub></mml:mtd><mml:mtd columnalign="left"><mml:mrow><mml:mo>:</mml:mo><mml:msubsup><mml:mrow><mml:mi mathvariant="script">P</mml:mi></mml:mrow><mml:mrow><mml:mi>i</mml:mi><mml:mo>-</mml:mo><mml:mn>1</mml:mn><mml:mo>,</mml:mo><mml:mi>j</mml:mi></mml:mrow><mml:mi>n</mml:mi></mml:msubsup><mml:mo>:</mml:mo><mml:mo>=</mml:mo><mml:mfrac><mml:mrow><mml:mi>&#x00394;</mml:mi><mml:mi>t</mml:mi></mml:mrow><mml:msup><mml:mrow><mml:mo stretchy="false">(</mml:mo><mml:mi>&#x00394;</mml:mi><mml:mi>x</mml:mi><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mn>2</mml:mn></mml:msup></mml:mfrac><mml:mfenced close="]" open="["><mml:msub><mml:mi>D</mml:mi><mml:mi>k</mml:mi></mml:msub><mml:mo>-</mml:mo><mml:mfrac><mml:msub><mml:mi>&#x003a6;</mml:mi><mml