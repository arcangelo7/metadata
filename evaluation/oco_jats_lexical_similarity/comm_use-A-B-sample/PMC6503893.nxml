				\setlength{\oddsidemargin}{-69pt}
				\begin{document}$$\begin{aligned} {\mathcal {R}}_0:=[0,{\mathcal {P}}_0); \ \ {\mathcal {R}}_p:=\left[ \sum _{q=0}^{p-1}{\mathcal {P}}_q,\sum _{q=0}^{p}{\mathcal {P}}_q\right) ,\ p= 1,2,3; \text { and } {\mathcal {R}}_4:=\left[ \sum _{q=0}^{3}{\mathcal {P}}_q,1\right] . \end{aligned}$$\end{document}</tex-math><mml:math id="M628" display="block"><mml:mrow><mml:mtable><mml:mtr><mml:mtd columnalign="right"><mml:mrow><mml:msub><mml:mi mathvariant="script">R</mml:mi><mml:mn>0</mml:mn></mml:msub><mml:mo>:</mml:mo><mml:mo>=</mml:mo><mml:mrow><mml:mo stretchy="false">[</mml:mo><mml:mn>0</mml:mn><mml:mo>,</mml:mo><mml:msub><mml:mi mathvariant="script">P</mml:mi><mml:mn>0</mml:mn></mml:msub><mml:mo stretchy="false">)</mml:mo></mml:mrow><mml:mo>&#x0037e;</mml:mo><mml:mspace width="4pt"/><mml:mspace width="4pt"/><mml:msub><mml:mi mathvariant="script">R</mml:mi><mml:mi>p</mml:mi></mml:msub><mml:mo>:</mml:mo><mml:mo>=</mml:mo><mml:mfenced close=")" open="["><mml:munderover><mml:mo>&#x02211;</mml:mo><mml:mrow><mml:mi>q</mml:mi><mml:mo>=</mml:mo><mml:mn>0</mml:mn></mml:mrow><mml:mrow><mml:mi>p</mml:mi><mml:mo>-</mml:mo><mml:mn>1</mml:mn></mml:mrow></mml:munderover><mml:msub><mml:mi mathvariant="script">P</mml:mi><mml:mi>q</mml:mi></mml:msub><mml:mo>,</mml:mo><mml:munderover><mml:mo>&#x02211;</mml:mo><mml:mrow><mml:mi>q</mml:mi><mml:mo>=</mml:mo><mml:mn>0</mml:mn></mml:mrow><mml:mi>p</mml:mi></mml:munderover><mml:msub><mml:mi mathvariant="script">P</mml:mi><mml:mi>q</mml:mi></mml:msub></mml:mfenced><mml:mo>,</mml:mo><mml:mspace width="4pt"/><mml:mi>p</mml:mi><mml:mo>=</mml:mo><mml:mn>1</mml:mn><mml:mo>,</mml:mo><mml:mn>2</mml:mn><mml:mo>,</mml:mo><mml:mn>3</mml:mn><mml:mo>&#x0037e;</mml:mo><mml:mspace width="0.333333em"/><mml:mtext>and</mml:mtext><mml:mspace width="0.333333em"/><mml:msub><mml:mi mathvariant="script">R</mml:mi><mml:mn>4</mml:mn></mml:msub><mml:mo>:</mml:mo><mml:mo>=</mml:mo><mml:mfenced close="]" open="["><mml:munderover><mml:mo>&#x02211;</mml:mo><mml:mrow><mml:mi>q</mml:mi><mml:mo>=</mml:mo><mml:mn>0</mml:mn></mml:mrow><mml:mn>3</mml:mn></mml:munderover><mml:msub><mml:mi mathvariant="script">P</mml:mi><mml:mi>q</mml:mi></mml:msub><mml:mo>,</mml:mo><mml:mn>1</mml:mn></mml:mfenced><mml:mo>.</mml:mo></mml:mrow></mml:mtd></mml:mtr></mml:mtable></mml:mrow></mml:math><graphic xlink:href="11538_2019_597_Article_Equ19.gif" position="anchor"/></alternatives></disp-formula></p></list-item><list-item><p id="Par101">At each grid point <inline-formula id="IEq305"><alternatives><tex-math id="M629">\documentclass[12pt]{minimal}
				\usepackage{amsmath}
