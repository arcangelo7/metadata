43a;&#x0043e;&#x00432;&#x0043e;&#x00439; &#x0043b;&#x0043e;&#x00433;&#x00438;&#x00441;&#x00442;&#x00438;&#x00447;&#x00435;&#x00441;&#x0043a;&#x0043e;&#x00439; &#x00440;&#x00435;&#x00433;&#x00440;&#x00435;&#x00441;&#x00441;&#x00438;&#x00438;.</p></sec><sec><title>&#x00420;&#x00435;&#x00437;&#x00443;&#x0043b;&#x0044c;&#x00442;&#x00430;&#x00442;&#x0044b;</title><p>&#x00418;&#x00437; 9&#x000a0;052&#x000a0;044&#x000a0;&#x00447;&#x00435;&#x0043b;&#x0043e;&#x00432;&#x00435;&#x0043a;, &#x00432;&#x0043a;&#x0043b;&#x0044e;&#x00447;&#x00435;&#x0043d;&#x0043d;&#x0044b;&#x00445; &#x00432;&#x0043e; &#x00432;&#x00437;&#x00432;&#x00435;&#x00448;&#x00435;&#x0043d;&#x0043d;&#x00443;&#x0044e; &#x00432;&#x0044b;&#x00431;&#x0043e;&#x00440;&#x0043a;&#x00443; &#x00438;&#x00437; &#x00433;&#x00435;&#x0043d;&#x00435;&#x00440;&#x00430;&#x0043b;&#x0044c;&#x0043d;&#x0043e;&#x00439; &#x00441;&#x0043e;&#x00432;&#x0043e;&#x0043a;&#x00443;&#x0043f;&#x0043d;&#x0043e;&#x00441;&#x00442;&#x00438;, 98,4% &#x00436;&#x00435;&#x0043d;&#x00449;&#x00438;&#x0043d; &#x0043f;&#x0043e;&#x0043b;&#x00443;&#x00447;&#x00430;&#x0043b;&#x00438; &#x00434;&#x0043e;&#x00440;&#x0043e;&#x00434;&#x0043e;&#x00432;&#x00443;&#x0044e; &#x0043f;&#x0043e;&#x0043c;&#x0043e;&#x00449;&#x0044c; &#x00432; &#x00442;&#x00435;&#x00447;&#x00435;&#x0043d;&#x00438;&#x00435; &#x0043f;&#x0043e;&#x00441;&#x0043b;&#x00435;&#x00434;&#x0043d;&#x00435;&#x00439; &#x00431;&#x00435;&#x00440;&#x00435;&#x0043c;&#x00435;&#x0043d;&#x0043d;&#x0043e;&#x00441;&#x00442;&#x00438;, &#x0043d;&#x0043e; &#x00442;&#x0043e;&#x0043b;&#x0044c;&#x0043a;&#x0043e; &#x00434;&#x0043b;&#x0044f; 71,5%&#x000a0;(95%-&#x00439; &#x00434;&#x0043e;&#x00432;&#x00435;&#x00440;&#x00438;&#x00442;&#x00435;&#x0043b;&#x0044c;&#x0043d;&#x0044b;&#x00439; &#x00438;&#x0043d;&#x00442;&#x00435;&#x00440;&#x00432;&#x00430;&#x0043b;: 69,7&#x02013;73,2) &#x0043c;&#x00430;&#x00442;&#x00435;&#x00440;&#x00435;&#x00439; &#x0044d;&#x00442;&#x00430; &#x0043c;&#x00435;&#x00434;&#x00438;&#x00446;&#x00438