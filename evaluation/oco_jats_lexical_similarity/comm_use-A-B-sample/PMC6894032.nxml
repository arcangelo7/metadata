="author"><name><surname>KarakasYesilkaya</surname><given-names>Nihan</given-names></name><xref ref-type="aff" rid="aff2">2</xref><role>MD</role></contrib><contrib contrib-type="author"><name><surname>Karaagac</surname><given-names>Erturk</given-names></name><xref ref-type="aff" rid="aff2">2</xref><role>MD</role></contrib><contrib contrib-type="author"><name><surname>Gokkurt</surname><given-names>Yasar</given-names></name><xref ref-type="aff" rid="aff2">2</xref><role>MD</role></contrib><contrib contrib-type="author"><name><surname>Iscan</surname><given-names>Sahin</given-names></name><xref ref-type="aff" rid="aff2">2</xref><role>MD</role></contrib><contrib contrib-type="author"><name><surname>Gurbuz</surname><given-names>Ali</given-names></name><xref ref-type="aff" rid="aff1">1</xref><role>Prof</role></contrib></contrib-group><aff id="aff1">
